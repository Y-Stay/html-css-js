body{
  margin:0 auto;
  padding:0;
  border:0;
  background-repeat:repeat-x;
  background-image:url(images/timg.png);
  font-family:Tahoma,Geneva,sans-serif;
}
#header a{
   text-decoration:none;
   line-height:61px;
   color:white;
   font-size:30px;
}
#header a:hover{
   font-weight:bold;
   text-decoration:underline;
   color:red;
}
div{

  margin:0 auto;
  padding:0;
}
#header,#footer{
   width:960px;
 }
#header{
   height:61px;
   background:url(images/top.png)repeat-x;
   text-align:right;
}
#navi{
   width:100%;
   height:91px;
   background:url(images/navi.png);
}
#main{
   border:5px solid #8ac1db;
   height:410px;
   width:950px;
   background-color:#e8f3f8;
}
#footer{
   height:50px;
}
#operate,#data,#pages{
   width:910px;
}
#operate{
   height:30px;
}
#data{
   height:340px;
}
#pages{
   height:28px;
}
#datalist{
   width:910px;
   margin:5px auto;
   text-align:center;
   background-color:#fff;
   border:0;
   border-collapse:collapse;
}
#datalist td{
   height:32px;
   vertical-align:middle;
   border:1px solid #CCC;
}
tr.header td{
   font-weight:bold;
   height:35px;
   background:#fbedce;
}
#datalist tr:hover{
   background-color:#ffff66;
}
#menu{
   margin:3px auto;
   width:960px;
   padding:5px;
   list-style-type:none;
}

#menu li{
   float:left;
   padding:0 14px;
}
#menu a{
 
   width:68px;
   height:77px;
}
.index_off{
   background:url(images/support.png) no-repeat;
}
.role_off{
   background:url(images/support.png) no-repeat;
}
.role_off{
   background:url(images/support.png) no-repeat;
}
